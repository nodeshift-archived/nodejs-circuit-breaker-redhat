{
  "name": "nodejs-circuit-breaker-redhat-name",
  "version": "2.0.0",
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "scripts": {
    "test": "tape test/integration/*.js | tap-spec",
    "lint": "xo",
    "prepare": "echo 'To confirm CVE compliance, run \"npm run security-check\"' ",
    "security-check": "nsp check",
    "coverage": "istanbul cover tape test/*.js",
    "dependencyCheck": "szero . --ci",
    "prepublish": "license-reporter report -s",
    "openshift": "nodeshift --strictSSL=false --dockerImage=registry.access.redhat.com/rhoar-nodejs/nodejs-10 --metadata.out=nodeshift-metadata.json",
    "postinstall": "license-reporter report -s && license-reporter save -s --xml licenses.xml",
    "start": "node ."
  },
  "main": "./bin/www",
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nodeshift-starters/nodejs-circuit-breaker-redhat.git"
  },
  "files": [
    "package.json",
    "LICENSE",
    "app.js",
    "lib",
    "bin",
    "licenses"
  ],
  "bugs": {
    "url": "https://github.com/nodeshift-starters/nodejs-circuit-breaker-redhat/issues"
  },
  "homepage": "https://github.com/nodeshift-starters/nodejs-circuit-breaker-redhat",
  "devDependencies": {
    "istanbul": "~0.4.5",
    "nodeshift": "~1.7.1",
    "nsp": "~2.6.2",
    "szero": "^0.7.1",
    "tap-spec": "~4.1.1",
    "tape": "~4.6.2",
    "xo": "~0.20.3"
  },
  "description": "",
  "keywords": [],
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "~2.8.4",
    "express": "4.16.0",
    "kube-probe": "~0.1.2",
    "license-reporter": "^1.2.0",
    "ws": "~3.3.2"
  }
}
